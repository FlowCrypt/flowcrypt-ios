{
  "ts-node": {
    "transpileOnly": true,
    "require": [
      "tsconfig-paths/register"
    ]
  },
  "compilerOptions": {
    "outDir": "./.tsbuild/",
    "lib": [
      "ES2022"
    ],
    "module": "commonjs",
    "target": "ES2022",
    "types": [
      "node",
      "@wdio/globals/types",
      "@wdio/jasmine-framework",
      "@wdio/junit-reporter",
      "@wdio/appium-service"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": false,
    "sourceMap": false,
    "checkJs": false,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "."
  },
  // "include": [
  //   "./api-mocks/**/*.ts",
  //   "./config/*.ts",
  //   "./tests/**/*.ts",
  // ]
}